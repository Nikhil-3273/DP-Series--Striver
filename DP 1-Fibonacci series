//fibonacci...
#include<bits/stdc++.h>
using namespace std;
#define ll long long int
// ll fib(ll n,vector<ll>&dp)
// {
//     if(n<=1) return 1;
//     if(dp[n]!=-1) return dp[n];
//     return dp[n]=fib(n-1,dp)+fib(n-2,dp);
// }
int main()
{
    ll n; //nth number..
    cin>>n;
    ll prev1=1, prev2=0;
    for(ll i=2; i<=n; i++)
    {
        ll cur=prev1+prev2;
        prev2=prev1; prev1=cur;
    }
    cout<<prev1<<endl;
    return 0;
}
